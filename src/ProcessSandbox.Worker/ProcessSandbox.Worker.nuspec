<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>ProcessSandbox.Worker</id>
        <version>0.11.0</version>
        <authors>Jonny Muir</authors>
        <license type="expression">MIT</license>
        <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
        <projectUrl>https://github.com/jonnymuir/ProcessSandbox</projectUrl>
        <description>Package Description</description>
        <copyright>Copyright Â© jonnymuir.com 2025</copyright>
        <tags>process isolation sandbox interop com native unmanaged proxy</tags>
        <repository type="git" url="https://github.com/jonnymuir/ProcessSandbox.git"
            branch="refs/heads/main" />

        <dependencies>
            <group targetFramework="net8.0">
                <dependency id="ProcessSandbox.Abstractions" version="0.11.0"
                    exclude="Build,Analyzers" />
                <dependency id="ProcessSandbox.Runner" version="0.11.0" exclude="Build,Analyzers" />
                <dependency id="MessagePack" version="3.1.4" exclude="Build,Analyzers" />
                <dependency id="Microsoft.Extensions.Logging.Abstractions" version="8.0.0"
                    exclude="Build,Analyzers" />
                <dependency id="Microsoft.Extensions.Logging.Console" version="8.0.0"
                    exclude="Build,Analyzers" />
            </group>
            <group targetFramework="net10.0">
                <dependency id="ProcessSandbox.Abstractions" version="0.11.0"
                    exclude="Build,Analyzers" />
                <dependency id="ProcessSandbox.Runner" version="0.11.0" exclude="Build,Analyzers" />
                <dependency id="MessagePack" version="3.1.4" exclude="Build,Analyzers" />
                <dependency id="Microsoft.Extensions.Logging.Abstractions" version="10.0.0"
                    exclude="Build,Analyzers" />
                <dependency id="Microsoft.Extensions.Logging.Console" version="10.0.0"
                    exclude="Build,Analyzers" />
            </group>
        </dependencies>
        <contentFiles> <!-- Include Assets as Content --> 
            <files include="any/**/*.*" buildAction="None" copyToOutput="true"/> 
        </contentFiles> 
    </metadata>
    <files>
        <file src="bin\$configuration$\net8.0\**" exclude="**\*.pdb;**\*.exe" target="lib\net8.0\" />
        <file src="bin\$configuration$\net10.0\**" exclude="**\*.pdb;**\*.exe" target="lib\net10.0\" />
        <file src="bin\$configuration$\net8.0\ProcessSandbox.Worker.runtimeconfig.json" target="contentFiles\any\net8.0\" />
        <file src="bin\$configuration$\net10.0\ProcessSandbox.Worker.runtimeconfig.json" target="contentFiles\any\net10.0\" />
    </files>
</package>